extends layout
block content
	.w3-container.w3-card-4
		header.w3-container.w3-light-blue
			h2 Listagem completa de Teses
		.w3-container.light-grey
			table.w3-table-all
				tr
					th Nome
					th No. Mec.
					th Curso
					th Instituicao
					th Título
					th Data
					th Grau de Ensino
				each tese in lista
					tr
						td=tese.nome
						td=tese.mecan
						td=tese.curso
						td=tese.inst
						td=tese.titulo
						td=tese.data
						td=tese.tipo
			form.w3-container(action='/',method='GET')
						input.w3-button.w3-block(type='submit',value='Voltar ao Menu Inicial')
		footer.w3-container.w3-light-blue
			address PRI2018 - António Chaves A75870	